#!/usr/bin/env php
<?php

// Option 1: Self-override check at the start
require_once __DIR__ . '/../override-helpers.php';
execute_override_if_exists(basename(__FILE__));

// If we got here, no override exists - run default implementation
echo "TASK-ALPHA (default): Running default implementation\n";
echo "TASK-ALPHA (default): MY_VAR=" . getenv('MY_VAR') . "\n";
